{
  "name": "chatter",
  "version": "0.0.1",
  "description": "real-time chat app with GIFs support",
  "dependencies": {
    "bcrypt": "5.0.0",
    "connect-flash": "0.1.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "mysql2": "2.1.0",
    "nodemon": "2.0.4",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "socket.io": "2.4.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "npx nodemon src/server.js"
  },
  "nodemonConfig": {
    "ignore": [
      "src/*",
      "node_modules/*"
    ],
    "delay": "0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/node": "7.10.5",
    "@babel/preset-env": "7.11.0"
  }
}
